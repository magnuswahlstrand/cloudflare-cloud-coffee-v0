{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types",
      "@cloudflare/workers-types/experimental",
      "@cloudflare/vitest-pool-workers",
      "cloudflare:test"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "include": [
      "src/**/*.ts",
      "tests/**/*.ts",
      "env.d.ts"
    ]
  }
}